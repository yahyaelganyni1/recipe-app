<p style="color: green"><%= notice %></p>

<h1>Shopping lists</h1>


<table class="table">
  <thead>
    <tr>
      <th scope="col">food</th>
      <th scope="col">quantity </th>
      <th scope="col">price</th>
    </tr>
  </thead>
  <tbody>
    <% @shopping_lists.each do |shopping_list| %>
      <td>
        <%= render shopping_list %>
      </td>
    <% end %>
    
    </tr>
   
  </tbody>
</table>